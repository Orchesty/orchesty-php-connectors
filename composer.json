{
    "name": "hanaboso/pipes-connectors",
    "description": "Connectors for PipesFramework",
    "version": "1.5.2",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "hanaboso/app-store": "^1.5",
        "hanaboso/pipes-php-sdk": "^1.5",
        "hanaboso/rabbit-mq-bundle": "^1.4"
    },
    "require-dev": {
        "aws/aws-sdk-php-symfony": "^2.2",
        "hanaboso/php-check-utils": "^1.1",
        "symfony/debug-bundle": "^5.1",
        "symfony/security-bundle": "^5.1",
        "symfony/serializer": "^5.1"
    },
    "suggest": {
        "aws/aws-sdk-php-symfony": "Install me, if you need to use Model\\Application\\Impl\\AmazonApps."
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\HbPFConnectors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HbPFConnectorsTests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ]
}
