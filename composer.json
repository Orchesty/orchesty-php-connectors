{
    "name": "hanaboso/pipes-connectors",
    "license": "proprietary",
    "description": "Connectors for PipesFramework",
    "version": "1.3.1",
    "autoload": {
        "psr-4": {
            "Hanaboso\\HbPFConnectors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HbPFConnectorsTests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pgsql": "*",
        "hanaboso/app-store": "^1.3",
        "hanaboso/rabbit-mq-bundle": "^1.3",
        "hanaboso/pipes-php-sdk": "^1.2"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.1"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "ralouphie/getallheaders": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ]
}
