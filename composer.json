{
    "name": "hanaboso/pipes-connectors",
    "license": "proprietary",
    "description": "Connectors for PipesFramework",
    "version": "1.2.0",
    "autoload": {
        "psr-4": {
            "Hanaboso\\HbPFConnectors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-pgsql": "*",
        "ext-pdo": "*",
        "hanaboso/app-store": "^1.2",
        "hanaboso/commons-bundle": "^1.3",
        "hanaboso/pipes-php-sdk": "^1.1"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ]
}
