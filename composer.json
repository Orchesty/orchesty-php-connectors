{
    "name": "hanaboso/pipes-connectors",
    "description": "Connectors for PipesFramework",
    "version": "1.4.7",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pgsql": "*",
        "hanaboso/app-store": "^1.4",
        "hanaboso/pipes-php-sdk": "^1.3",
        "hanaboso/rabbit-mq-bundle": "^1.3"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\HbPFConnectors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HbPFConnectorsTests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ]
}
